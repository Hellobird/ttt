<template name="1ac0162e">
  <view class="_div data-v-105eb302 cancel-order-modal">
    <view class="_div data-v-105eb302 cancel-order-condition" wx:key="index" key="{{index}}" wx:for="{{confirmPlanlist}}" wx:for-index="index" wx:for-item="list">
      <view class="_div data-v-105eb302 cancel-order-title" wx:if="{{list.type}}"> {{list.type}} </view>
      <view class="_div data-v-105eb302 cancel-statement-wrap">
        <view class="_div data-v-105eb302 cancel-radio-wrap" wx:key="priceIndex" key="{{priceIndex}}" wx:for="{{list.prices}}" wx:for-index="priceIndex" wx:for-item="item">
          <label class="_span data-v-105eb302 cancel-radio"></label>
          <label class="_span data-v-105eb302 cancel-label">{{item.name+""+item.price}}</label>
        </view>
      </view>
    </view>
    <view class="_div data-v-105eb302 cancel-plan-wrap">
      <button class="_button data-v-105eb302 cancel-plan">放弃维修</button>
      <button class="_button data-v-105eb302 cancel-plan" bindtap="handleProxy" data-eventid="{{'mSz-0'}}" data-comkey="{{$k}}">重新申报方案</button>
    </view>
    <view class="_div data-v-105eb302 agree-plan-wrap">
      <button class="_button data-v-105eb302 cancel-plan agree-plan" bindtap="handleProxy" data-eventid="{{'PWC-1'}}" data-comkey="{{$k}}">同意方案</button>
    </view>
  </view>
</template>