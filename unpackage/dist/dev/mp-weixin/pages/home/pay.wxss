
.adressli {
	padding: 30px;
	font-size: 26px;
	border-bottom: 1px solid #E5E5E5;
	line-height: 26px;
}
.adressli ._div>._div {
	display: inline-block;
}
.adressli>._div:nth-child(1) {
	font-size: 30px;
	padding: 0 30px;
}
.adressli>._div:nth-child(2) {
	margin-bottom: 30px;
	margin-top: 25px;
	padding: 0 30px;
}
.adressinf {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	font-size: 24px;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.adressinf image {
	width: 20px;
	height: 30px;
}
.adressinf ._div {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	line-height: 30px;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.adressinf ._div image {}
.adressinf ._div ._span {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	margin-left: 10px;
}
.payinf {
	padding: 0 30px;
}
.payinf ._h1 {
	font-size: 28px;
	line-height: 80px;
}
.payinf>._div {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	font-size: 24px;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	margin-bottom: 20px;
}
.payinf>._div image {
	width: 300px;
	height: 200px;
}
.payinf>._div ._div {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
}
.payinf>._div ._div ._span {
	margin-left: 20px;
}
.payinf>._div ._div ._span:last-child {
	color: #FEC200;
}
.tip {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	padding: 0 30px;
	line-height: 40px;
	margin-top: 10px;
	font-size: 24px;
}
.tip ._div:last-child {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	margin-left: 10px;
}
.apply {
	position: fixed;
	bottom: 0;
	width: 100%;
	height: 100px;
	text-align: center;
}
.apply ._div {
	margin-left: 30px;
	margin-right: 30px;
	height: 100px;
	line-height: 100px;
	color: white;
	background: #F6C11B;
	font-size: 28px;
}
.time {
	border-bottom: 1px solid #E5E5E5;
	border-top: 1px solid #E5E5E5;
}
.time>._p {
	text-align: center;
}
.time>._p view {
	display: inline-block;
	vertical-align: top;
	text-align: center;
	line-height: 100px;
	font-size: 24px;
	margin-right: 20px;
	position: relative;
}
.time ._p view:nth-child(1) ._span:nth-child(2n+1) {
	color: #FEC200;
}
.center{
	text-align: center;
}

