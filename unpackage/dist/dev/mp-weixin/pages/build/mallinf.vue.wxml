<template name="044b26dd">
  <view class="_div data-v-07f02cfd">
    <view class="_div data-v-07f02cfd mallinf1">
      <view class="_div data-v-07f02cfd otherinf">
        <view class="_div data-v-07f02cfd mallname"></view>
      </view>
      <image src="../../static/build/mall.jpg" class="_image data-v-07f02cfd mallimage"></image>
    </view>
    <view class="_div data-v-07f02cfd mallinf2">
      <view class="_div data-v-07f02cfd mallinf2price">
        <label class="_span data-v-07f02cfd">￥{{mallinf.price}}</label>
        <image src="../../static/close.png" class="_image data-v-07f02cfd"></image>
      </view>
      <view class="_div data-v-07f02cfd">多类牌合页</view>
      <view class="_div data-v-07f02cfd mallinf2s">
        <label class="_span data-v-07f02cfd">月销：50笔</label>
        <label class="_span data-v-07f02cfd">评价﹀</label>
        <label class="_span data-v-07f02cfd">北京通州</label>
      </view>
    </view>
    <view class="_div data-v-07f02cfd bggray"></view>
    <view class="_div data-v-07f02cfd guige">
      <view class="_h1 data-v-07f02cfd">规格</view>
      <view class="_div data-v-07f02cfd guigetype">
        <label wx:key="index" key="{{index}}" bindtap="handleProxy" data-eventid="{{'Exk-0-'+index}}" data-comkey="{{$k}}" wx:for="{{guigetype}}" wx:for-index="index" wx:for-item="item" class="_span data-v-07f02cfd {{[type == index ? 'active' : '']}}">{{item.name}}</label>
      </view>
      <view class="_div data-v-07f02cfd add">
        <view class="_div data-v-07f02cfd addedit">
          <image src="../../static/build/minus.png" catchtap="handleProxy" data-eventid="{{'qhS-1'}}" data-comkey="{{$k}}" class="_image data-v-07f02cfd"></image>
          <label class="_span data-v-07f02cfd">{{mallinf.mallnum}}</label>
          <image src="../../static/build/add.png" catchtap="handleProxy" data-eventid="{{'25o-2'}}" data-comkey="{{$k}}" class="_image data-v-07f02cfd"></image>
        </view>
        <label class="_span data-v-07f02cfd">数量</label>
      </view>
    </view>
    <view class="_div data-v-07f02cfd bggray"></view>
    <view class="_div data-v-07f02cfd footer">
      <view class="_div data-v-07f02cfd edit">
        <label class="_span data-v-07f02cfd">加入购物车</label>
        <label bindtap="handleProxy" data-eventid="{{'KHT-3'}}" data-comkey="{{$k}}" class="_span data-v-07f02cfd">购买</label>
      </view>
      <label class="_span data-v-07f02cfd mallnum" hidden="{{!(mallnum)}}">{{mallnum}}</label>
      <image src="../../static/logo.jpg" class="_image data-v-07f02cfd logo" bindtap="handleProxy" data-eventid="{{'qio-4'}}" data-comkey="{{$k}}"></image>
      <label class="_span data-v-07f02cfd money" hidden="{{!(mallnum)}}">￥{{mallprice}}</label>
    </view>
    <view class="_div data-v-07f02cfd pop" wx:if="{{pop}}" bindtap="handleProxy" data-eventid="{{'Hnh-9'}}" data-comkey="{{$k}}">
      <view class="_div data-v-07f02cfd popcont">
        <view class="_div data-v-07f02cfd delbar">
          <image src="../../static/mine/del.png" catchtap="handleProxy" data-eventid="{{'9IG-5'}}" data-comkey="{{$k}}" class="_image data-v-07f02cfd"></image>
          <label catchtap="handleProxy" data-eventid="{{'mgt-6'}}" data-comkey="{{$k}}" class="_span data-v-07f02cfd">清空购物车</label>
        </view>
        <view class="_div data-v-07f02cfd barlist">
          <view class="_div data-v-07f02cfd add" wx:key="index" key="{{index}}" wx:for="{{barlist}}" wx:for-index="index" wx:for-item="item">
            <view class="_div data-v-07f02cfd addedit">
              <image src="../../static/build/minus.png" catchtap="handleProxy" data-eventid="{{'xw2-7-'+index}}" data-comkey="{{$k}}" class="_image data-v-07f02cfd"></image>
              <label class="_span data-v-07f02cfd">{{item.mallnum}}</label>
              <image src="../../static/build/add.png" catchtap="handleProxy" data-eventid="{{'96B-8-'+index}}" data-comkey="{{$k}}" class="_image data-v-07f02cfd"></image>
            </view>
            <label class="_span data-v-07f02cfd barname">{{item.name}}</label>
            <label class="_span data-v-07f02cfd barmoney">￥{{item.price}}</label>
          </view>
        </view>
      </view>
    </view>
  </view>
</template>