<template name="018944b8">
  <view class="_div data-v-43e65ad8">
    <view class="_div data-v-43e65ad8 adress">
      <view class="_div data-v-43e65ad8 adressli" bindtap="handleProxy" wx:if="{{adress}}" data-eventid="{{'Nfw-1'}}" data-comkey="{{$k}}">
        <view class="_div data-v-43e65ad8">姓      名：{{adress.name}}</view>
        <view class="_div data-v-43e65ad8">联系方式：{{adress.phone}}</view>
        <view class="_div data-v-43e65ad8 adressinf">
          <view class="_div data-v-43e65ad8">
            <view class="_div data-v-43e65ad8"></view>
            <label class="_span data-v-43e65ad8">收货地址：{{adress.provinceName+adress.countyAreaName+adress.cityName+adress.address}}</label>
          </view>
          <image src="../../static/right.jpg" class="_image data-v-43e65ad8"></image>
        </view>
      </view>
      <view class="_div data-v-43e65ad8 adressli center" bindtap="handleProxy" wx:else data-eventid="{{'2Z4-0'}}" data-comkey="{{$k}}"> 请选择地址 </view>
    </view>
    <view class="_div data-v-43e65ad8 payinf inff" wx:key="list" key="{{list}}" wx:for="{{buildinf}}" wx:for-index="index" wx:for-item="list">
      <view class="_div data-v-43e65ad8">
        <image src="{{static+list.picture}}" class="_image data-v-43e65ad8"></image>
        <view class="_div data-v-43e65ad8">
          <view class="_p data-v-43e65ad8">
            <view class="_view data-v-43e65ad8">{{name}}</view>
            <view class="_view data-v-43e65ad8">
              <label class="_span data-v-43e65ad8">规格</label>
              <label class="_span data-v-43e65ad8">{{list.name}}</label>
            </view>
            <view class="_view data-v-43e65ad8">
              <label class="_span data-v-43e65ad8">数量</label>
              <label class="_span data-v-43e65ad8">x{{list.num}}</label>
            </view>
          </view>
        </view>
      </view>
    </view>
    <view class="_div data-v-43e65ad8 payinf time">
      <view class="_h1 data-v-43e65ad8">配送时间选择</view>
      <view class="_p data-v-43e65ad8">
        <view class="_view data-v-43e65ad8">
          <picker mode="date" value="日期" custom-item="{{customItem}}" class="_picker data-v-43e65ad8 regionpicker" wx:if="{{!disabled}}" bindchange="handleProxy" data-eventid="{{'MRO-2'}}" data-comkey="{{$k}}">
            <view class="_view data-v-43e65ad8 picker"> 日期 </view>
          </picker>
          <label wx:if="{{!date}}" class="_span data-v-43e65ad8">请选择日期</label>
          <view wx:else class="_view data-v-43e65ad8">
            <label class="_span data-v-43e65ad8">{{y}}</label>
            <label class="_span data-v-43e65ad8">年</label>
            <label class="_span data-v-43e65ad8">{{m}}</label>
            <label class="_span data-v-43e65ad8">月</label>
            <label class="_span data-v-43e65ad8">{{d}}</label>
            <label class="_span data-v-43e65ad8">日</label>
          </view>
        </view>
        <view class="_view data-v-43e65ad8">
          <picker mode="time" value="时间" custom-item="{{customItem}}" class="_picker data-v-43e65ad8 regionpicker" wx:if="{{!disabled}}" bindchange="handleProxy" data-eventid="{{'5mZ-3'}}" data-comkey="{{$k}}">
            <view class="_view data-v-43e65ad8 picker"> 时间 </view>
          </picker>
          <label class="_span data-v-43e65ad8"> {{time||'请选择时间'}} </label>
        </view>
      </view>
    </view>
    <view class="_div data-v-43e65ad8 payinf sendtype">
      <view class="_h1 data-v-43e65ad8">配送方式</view>
      <view wx:key="index" key="{{index}}" bindtap="handleProxy" data-eventid="{{'N2W-4-'+index}}" data-comkey="{{$k}}" wx:for="{{vehiclelist}}" wx:for-index="index" wx:for-item="list" class="_p data-v-43e65ad8">
        <label class="_span data-v-43e65ad8 {{[index == vehicleindex ? 'active' : '']}}">{{list.name}}</label>
        <label class="_span data-v-43e65ad8">起步价￥{{list.startPrice}}</label>
      </view>
    </view>
    <view class="_div data-v-43e65ad8 payinf sendtype sendtype1">
      <view class="_h1 data-v-43e65ad8">是否搬运上楼</view>
      <view class="_p data-v-43e65ad8 carry">
        <image src="../../static/mine/uncheck.jpg" wx:if="{{requireCarry==1}}" class="_image data-v-43e65ad8"></image>
        <image src="../../static/mine/check.jpg" wx:else bindtap="handleProxy" data-eventid="{{'na0-5'}}" data-comkey="{{$k}}" class="_image data-v-43e65ad8"></image>
        <label class="_span data-v-43e65ad8">是</label>
        <image src="../../static/mine/uncheck.jpg" wx:if="{{requireCarry==2}}" class="_image data-v-43e65ad8"></image>
        <image src="../../static/mine/check.jpg" wx:else bindtap="handleProxy" data-eventid="{{'qvu-6'}}" data-comkey="{{$k}}" class="_image data-v-43e65ad8"></image>
        <label class="_span data-v-43e65ad8">否</label>
      </view>
      <view class="_p data-v-43e65ad8 floor">
        <view wx:key="index" key="{{index}}" bindtap="handleProxy" data-eventid="{{'9aA-7-'+index}}" data-comkey="{{$k}}" wx:for="{{carrylist}}" wx:for-index="index" wx:for-item="list" class="_view data-v-43e65ad8">
          <view class="_view data-v-43e65ad8 {{[index == carryindex ? 'active' : '']}}"></view>
          <view class="_view data-v-43e65ad8">{{list.name}}</view>
        </view>
      </view>
      <view class="_p data-v-43e65ad8 louceng" wx:if="{{carrylist.length}}">
        <input placeholder="请输入楼层" value="{{floor}}" bindinput="handleProxy" type="number" data-eventid="{{'KnJ-8'}}" data-comkey="{{$k}}" class="_input data-v-43e65ad8" />层 起步价{{carrylist[carryindex].startPrice}} </view>
    </view>
    <view class="_div data-v-43e65ad8 payinf remake">
      <view class="_h1 data-v-43e65ad8">订单备注</view>
      <textarea value="{{remark}}" bindinput="handleProxy" data-eventid="{{'tYQ-9'}}" data-comkey="{{$k}}" class="_textarea data-v-43e65ad8" />
    </view>
    <view class="_div data-v-43e65ad8 payinf mingxi">
      <view class="_h1 data-v-43e65ad8">价格明细</view>
      <view class="_div data-v-43e65ad8 mingxiinf">
        <view class="_p data-v-43e65ad8">货款：{{mallprice}}</view>
        <view class="_p data-v-43e65ad8">配送费：{{sendprice}}</view>
        <view class="_p data-v-43e65ad8">搬运费：{{carryprice}}</view>
      </view>
    </view>
    <view class="_div data-v-43e65ad8 bgheight"></view>
    <view class="_div data-v-43e65ad8 apply">
      <label class="_span data-v-43e65ad8">合计：
        <label class="_span data-v-43e65ad8">{{allprice}}</label>
      </label>
      <label bindtap="handleProxy" data-eventid="{{'fSs-10'}}" data-comkey="{{$k}}" class="_span data-v-43e65ad8">确认支付</label>
    </view>
  </view>
</template>