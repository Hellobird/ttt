<template name="3efe0f5c">
  <view class="_view data-v-517cb632">
    <view class="_div data-v-517cb632 shopping-car">
      <view class="_div data-v-517cb632 shopping-options-wrap">
        <view class="_ul data-v-517cb632 shopping-options">
          <view class="_li data-v-517cb632 shopping-option-item shopping-collect">移入购物车</view>
          <view class="_li data-v-517cb632 shopping-option-item">删除</view>
        </view>
      </view>
      <view class="_div data-v-517cb632 shopping-goods-container">
        <view class="_div data-v-517cb632 shopping-goods-list" wx:key="index" key="{{index}}" wx:for="{{list}}" wx:for-index="index" wx:for-item="list">
          <view class="_div data-v-517cb632 shopping-seller">{{list.name}}</view>
          <view class="_div data-v-517cb632 shopping-goods-item" wx:key="itemindex" key="{{itemindex}}" wx:for="{{list.mall}}" wx:for-index="itemindex" wx:for-item="item">
            <radio class="_input data-v-517cb632" />
            <view class="_div data-v-517cb632 shopping-goods-img">
              <image src="{{item.picture}}" class="_image data-v-517cb632"></image>
            </view>
            <view class="_div data-v-517cb632 shopping-good-info">
              <view class="_p data-v-517cb632 shopping-good-name">{{item.name}}</view>
              <view class="_div data-v-517cb632 shopping-good-type" wx:if="{{item.type}}">规格: {{item.type}}</view>
              <view class="_div data-v-517cb632 shopping-good-price">
                <label class="_span data-v-517cb632">¥ {{item.price}}</label>
                <view class="_ul data-v-517cb632 shopping-num-container">
                  <view class="_li data-v-517cb632 shopping-option" bindtap="handleProxy" data-eventid="{{'pPb-0-'+index+'-'+itemindex}}" data-comkey="{{$k}}">-</view>
                  <view class="_li data-v-517cb632">{{item.num}}</view>
                  <view class="_li data-v-517cb632 shopping-option shopping-add" bindtap="handleProxy" data-eventid="{{'JqE-1-'+index+'-'+itemindex}}" data-comkey="{{$k}}">+</view>
                </view>
              </view>
            </view>
          </view>
        </view>
      </view>
    </view>
  </view>
</template>