<template name="3efe0f5c">
  <view class="_view data-v-517cb632">
    <view class="_div data-v-517cb632 shopping-car">
      <view class="_div data-v-517cb632 shopping-options-wrap">
        <view class="_ul data-v-517cb632 shopping-options">
          <view class="_li data-v-517cb632 shopping-option-item shopping-collect" bindtap="handleProxy" data-eventid="{{'Abs-0'}}" data-comkey="{{$k}}">删除</view>
        </view>
      </view>
      <view class="_div data-v-517cb632 shopping-goods-container">
        <view class="_div data-v-517cb632 shopping-goods-list" wx:key="list" key="{{list}}" wx:for="{{list}}" wx:for-index="index" wx:for-item="list">
          <view class="_div data-v-517cb632 shopping-seller">{{list.storename}}</view>
          <view class="_div data-v-517cb632 shopping-goods-item" wx:key="item" key="{{item}}" wx:for="{{list.cartlist}}" wx:for-index="itemindex" wx:for-item="item">
            <radio readonly="true" wx:if="{{!item.checked}}" bindtap="handleProxy" data-eventid="{{'7R2-2-'+index+'-'+itemindex}}" data-comkey="{{$k}}" class="_input data-v-517cb632" />
            <radio checked wx:else bindtap="handleProxy" data-eventid="{{'w2d-1-'+index+'-'+itemindex}}" data-comkey="{{$k}}" class="_input data-v-517cb632" />
            <view class="_div data-v-517cb632 shopping-goods-img">
              <image src="{{static+item.picture}}" class="_image data-v-517cb632 noimage"></image>
            </view>
            <view class="_div data-v-517cb632 shopping-good-info">
              <view class="_p data-v-517cb632 shopping-good-name">{{item.goodsName}}</view>
              <view class="_div data-v-517cb632 shopping-good-type" wx:if="{{item.goodsSpecName}}">规格: {{item.goodsSpecName}}</view>
              <view class="_div data-v-517cb632 shopping-good-price">
                <label class="_span data-v-517cb632">¥ {{item.price}}</label>
                <view class="_ul data-v-517cb632 shopping-num-container">
                  <view class="_li data-v-517cb632 shopping-option" bindtap="handleProxy" data-eventid="{{'PX8-3-'+index+'-'+itemindex}}" data-comkey="{{$k}}">-</view>
                  <view class="_li data-v-517cb632">{{item.num}}</view>
                  <view class="_li data-v-517cb632 shopping-option shopping-add" bindtap="handleProxy" data-eventid="{{'Gyv-4-'+index+'-'+itemindex}}" data-comkey="{{$k}}">+</view>
                </view>
              </view>
            </view>
          </view>
          <view class="_div data-v-517cb632 shopping-all" wx:if="{{list.allnum}}">
            <label class="_span data-v-517cb632">合计：</label>
            <label class="_span data-v-517cb632">￥{{list.allmoneynum}}</label>
            <label bindtap="handleProxy" data-eventid="{{'HOU-5-'+index}}" data-comkey="{{$k}}" class="_span data-v-517cb632">结算（{{list.allnum}}）</label>
          </view>
        </view>
      </view>
    </view>
  </view>
</template>